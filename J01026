import java.util.*;
public class SoChinhPhuong {
    public static void main(String[] args){
        Scanner in= new Scanner(System.in);
        int t= in.nextInt();
        while(t-->0){
            int n=in.nextInt();
            int k=(int)Math.sqrt(n);
            if(k*k==n) System.out.println("YES");
            else System.out.println("NO");
        }
    }
}
